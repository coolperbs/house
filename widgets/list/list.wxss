


/**.tpl-listitem st**/
.tpl-listitem.xiaoqu{
	background-color:white;

}
.tpl-listitem.xiaoqu .listitem-inner{
	height:160rpx;
	background-color:white;
	margin-left:30rpx;
	padding:30rpx 0 30rpx 0;
	border-bottom:1px solid #f1f1f1;
	display: flex;
}
.tpl-listitem.xiaoqu .listitem-inner .left{
	width:212rpx;
}
.tpl-listitem.xiaoqu .listitem-inner .left image{
	width: 212rpx;
	height: 160rpx;
}
.tpl-listitem.xiaoqu .listitem-inner .right{
	margin-left:20rpx;
	display:flex;
	position:relative;
	width:450rpx;
}
.tpl-listitem.xiaoqu .listitem-inner .right .info{
	display: flex;
	flex-direction: column;
}
.tpl-listitem.xiaoqu .listitem-inner .right .info .name{
	font-size:14px;
	color:#565656;
	margin-bottom:20rpx;
}
.tpl-listitem.xiaoqu .listitem-inner .right .info .region{
	font-size:12px;
	color:#b6b6b6;
	margin-bottom:20rpx;
}
.tpl-listitem.xiaoqu .listitem-inner .right .info .type{
	font-size:12px;
	color:#b6b6b6;
}
.tpl-listitem.xiaoqu .listitem-inner .right .info .price{
	font-size:12px;
	color:#a4a4a4;
}
.tpl-listitem.xiaoqu .listitem-inner .right .price{
	position: absolute;
	right: 0;
	color:#cf5a26;
}
.tpl-listitem.xiaoqu .listitem-inner .right .price .num{
	font-size: 13px
}
.tpl-listitem.xiaoqu .listitem-inner .right .price .YP{
	font-size: 14px
}
/**.tpl-listitem.xiaoqu ed**/



